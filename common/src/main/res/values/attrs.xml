<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FlashView">
        <attr name="effect">
            <enum name="accordion" value="0" />
            <enum name="cube" value="1" />
            <enum name="def" value="2" />
            <enum name="depthPage" value="3" />
            <enum name="inRightDown" value="4" />
            <enum name="inRightUp" value="5" />
            <enum name="rotate" value="6" />
            <enum name="zoomOutPage" value="7" />

        </attr>
    </declare-styleable>

    <declare-styleable name="IRecyclerView">
        <attr name="refreshHeaderLayout" format="reference" />
        <attr name="loadMoreFooterLayout" format="reference" />
        <attr name="refreshEnabled" format="boolean" />
        <attr name="loadMoreEnabled" format="boolean" />
        <attr name="refreshFinalMoveOffset" format="dimension" />
    </declare-styleable>
</resources>